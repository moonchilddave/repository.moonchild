<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="plugin.program.moonchildwizard" name="Moonchild Wizard" version="1.0.2" provider-name="Moonchild">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>executable</provides>
  </extension>
  <extension point="xbmc.service" library="startup.py" start="startup" />
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Moonchild Wizard</summary>
    <description lang="en">Moonchild Wizard allows you to get access to the latest Moonchild Custom Builds.</description>
    <platform>all</platform>
  </extension>
</addon>